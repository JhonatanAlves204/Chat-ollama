@page
@model Chat_Ollama.Pages.ResumoDocModel
@{
    ViewData["Title"] = "Resumo de Doc";
}
<head>
    <link rel="stylesheet" href="~/css/Default.css" asp-append-version="true" />
</head>
<main>
    <section class="main-chat">
        <div class="chat-ollama">
            <div class="user"></div>
            <div class="bot"></div>
        </div>
    </section>
    <section>
        <div>
            <div class="display-submit">
                <input type="text" placeholder="Envie o Documento Para o Ollama">
                <a><img src="./imgs/next.png" alt=""></a>
            </div>
        </div>
    </section>
</main>